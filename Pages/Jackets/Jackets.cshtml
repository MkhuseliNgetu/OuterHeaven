@model OuterHeaven.Pages.JacketModel
@{
    ViewData["Title"] = "Jackets";
}
<header>
   <link rel="stylesheet" href="../css/HeadersAndButtons.css" type="text/css"/>
   <link rel="stylesheet" href="../css/CategoryItems.css" type="text/css" />
   <link rel="stylesheet" href="../css/CategoryItemButtons.css" type="text/css" />
   <link rel="stylesheet" href="../css/ItemEffects.css" type="text/css" />
